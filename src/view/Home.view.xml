<mvc:View
	controllerName="com.ts.tsReport.controller.Home"
	displayBlock="true"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc">

    <MessagePage
        title="{i18n>app_title}"
        text="{i18n>app_title}"
        icon="sap-icon://accept"
        id="page"
        description="{i18n>app_description}">
        <buttons>
            <Button
                text="{i18n>btn_text}"
                press="sayHello" />
        </buttons>
    </MessagePage>

</mvc:View>